{
  "name": "bencode2json",
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > public/bundle.js",
    "start": "ecstatic -p 8000 public",
    "watch": "watchify main.js -o public/bundle.js -dv"
  },
  "dependencies": {
    "bencode": "^0.12.0",
    "browserify": "^14.4.0",
    "ecstatic": "~2.1.0",
    "react": "~15.5.4",
    "reactify": "^1.1.1",
    "uglify-js": "^3.0.15",
    "watchify": "^3.2.3"
  },
  "license": "MIT"
}
